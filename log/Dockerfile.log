FROM golang:1.20.5 as builder

WORKDIR /src
COPY . .
ARG TARGETARCH
ARG TARGETOS

#RUN make log-build TARGETARCH=$TARGETARCH TARGETOS=$TARGETOS
RUN go get && CGO_ENABLED=0 GOOS=${TARGETOS} GOARCH=${TARGETARCH} go build -v -o log


FROM scratch
WORKDIR /
COPY --from=builder /src/log .
ENTRYPOINT ["/log"]
EXPOSE 8000